{
  "name": "hierarchical-sorting",
  "version": "1.0.0",
  "description": "Sorting methods to Specific data set",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:watch": "nodemon",
    "try:simple": "ts-node src/index.ts",
    "try:simpleMetric": "ts-node src/try-simple-metric.ts",
    "try:multipleMetric": "ts-node src/try-multiple-metrics.ts",
    "try:functionMetric": "ts-node src/try-function-metrics.ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "check": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix && npm run format",
    "test": "jest -c jest.config.js --verbose",
    "test:watch": "jest -c jest.config.js --watch --verbose",
    "test:cov": "jest -c jest.config.js --coverage",
    "commit": "npm run lint && git-cz"
  },
  "author": "joaocasalnovo@gmail.com",
  "license": "ISC",
  "dependencies": {
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "commitizen": "^4.2.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "husky": "^4.3.8",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "prettier": "1.19.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
